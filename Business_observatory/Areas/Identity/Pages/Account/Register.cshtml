<!-- My Account Section Start -->
<div class="rs-my-account pt-120 pb-120 md-pt-80 md-pb-80">
    <div class="container">
        <div class="row justify-content-center align-items-center">
            <div class="col-lg-6">
                <h2 class="title pb-30 md-pb-15">Register</h2>
                <div class="rs-login">
                    <form id="registerForm" asp-route-returnUrl="@Model.ReturnUrl" method="post">
                        <div class="form-group">
                            <label>Email<span>*</span></label>
                            <input asp-for="Input.Email" id="gname"  class="form-control-mod" type="text" required="">
                            <label>contraña <span>*</span></label>
                            <input asp-for="Input.Password" id="tname" class="form-control-mod" type="password" required="">
                            <label>confirmar contraseña <span>*</span></label>
                            <input asp-for="Input.ConfirmPassword" id="tname" class="form-control-mod" type="password" required="">
                        </div>
                        <p></p>
                        <p>
                        </p>
                        <button  class="add-btn" type="submit">Registrar</button>
                    </form>
                </div>
                <div class="">
                    <section>
                        <h3></h3>
                        <hr />
                        @{
                            if ((Model.ExternalLogins?.Count ?? 0) == 0)
                            {
                                <div>
                                    <p>
                                        
                                    </p>
                                </div>
                            }
                            else
                            {
                                <form id="external-account" asp-page="./ExternalLogin" asp-route-returnUrl="@Model.ReturnUrl" method="post" class="form-horizontal">
                                    <div>
                                        <p>
                                            @foreach (var provider in Model.ExternalLogins!)
                                            {
                                                <button type="submit" class="btn btn-primary" name="provider" value="@provider.Name" title="Log in using your @provider.DisplayName account">@provider.DisplayName</button>
                                            }
                                        </p>
                                    </div>
                                </form>
                            }
                        }
                    </section>
                </div>
            </div>
        </div>
    </div>
</div>
