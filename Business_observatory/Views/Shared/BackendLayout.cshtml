                            <li>
                                <a asp-action="Index" asp-controller="Categorias">Lista de Categorias</a>
                            </li>
                            <li>
                                <a asp-action="Create" asp-controller="Categorias">Crear Categoria</a>
                            </li>
                                             
                        </ul>
                    </li>

                    <li class="menu">
